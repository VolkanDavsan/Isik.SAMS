                                    <hr>
                                    <div class="clearfix">
                                        <div class="pull-left"><strong>Country of High School</strong></div>
                                        <div class="pull-right">@Model.HighSchoolCountry</div>
                                    </div>
                                    <hr>
                                    <div class="clearfix">
                                        <div class="pull-left"><strong>City of High School</strong></div>
                                        <div class="pull-right">@Model.HighSchoolCity</div>
                                    </div>
                                    <hr>
                                    <div class="clearfix">
                                        <div class="pull-left"><strong>Language Exam Score</strong></div>
                                        <div class="pull-right">@Model.LanguageExamScore</div>
                                    </div>
                                    <hr>
                                    <div class="clearfix">
                                        <div class="pull-left"><strong>Language Proficiency</strong></div>
                                        <div class="pull-right">@Model.LanguageProficiency</div>
                                    </div>
                                    <hr>

                                </div>
                            </div>
                        }
                        @if (Model.ProgramId == 6003)
                        {
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="clearfix">
                                        <div class="pull-left"><strong>Graduation Year of Master</strong></div>
                                        <div class="pull-right">@Model.MasterGradDate</div>
                                    </div>
                                    <hr>
                                    <div class="clearfix">
                                        <div class="pull-left"><strong>GPA of Master</strong></div>
                                        <div class="pull-right">@Model.MasterGPA</div>
                                    </div>
                                    <hr>
                                    <div class="clearfix">
                                        <div class="pull-left"><strong>Country of Master</strong></div>
                                        <div class="pull-right">@Model.MasterCountry</div>
                                    </div>
                                    <hr>
                                    <div class="clearfix">
                                        <div class="pull-left"><strong>Master University Name</strong></div>
                                        <div class="pull-right">@Model.MasterUni</div>
                                    </div>
                                    <hr>
                                    <div class="clearfix">
                                        <div class="pull-left"><strong>Master Program Name</strong></div>
                                        <div class="pull-right">@Model.MasterProgram</div>
                                    </div>
                                    <hr>
                                    <div class="clearfix">
                                        <div class="pull-left"><strong>Language Proficiency</strong></div>
                                        <div class="pull-right">@Model.LanguageProficiency</div>
                                    </div>
                                    <hr>
                                </div>
                            </div>
                        }
                        @if (Model.ProgramId == 6002)
                        {
                            <div class="row">
                                <div class="col-md-6">
                                    <div class="clearfix">
                                        <div class="pull-left"><strong>Graduation Year of Bachelor</strong></div>
                                        <div class="pull-right">@Model.BachelorGradDate</div>
                                    </div>
                                    <hr>
                                    <div class="clearfix">
                                        <div class="pull-left"><strong>GPA of Bachelor</strong></div>
                                        <div class="pull-right">@Model.BachelorGPA</div>
