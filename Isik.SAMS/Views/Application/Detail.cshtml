<script src="/Assets/lib/sweetalerts/sweetalert.js"></script>
<script type="text/javascript">
    function approveApplication(id) {
        swal({
            title: "Are you sure?",
            text: "Are you sure that you want to approve this application?",
            type: "warning",
            showCancelButton: true,
            confirmButtonText: "Yes, approve it!",
            confirmButtonColor: "#ec6c62"
        }).then((result) => {
            if (result) {
                $.ajax({
                    dataType: 'JSON',
                    type: 'POST',
                    async: false,
                    timeout: 600000,
                    url: "/Application/Approval?id=" + id,
                    success: function () {
                        showApprovalMessage();
                    },
                    error: function () {
                        swal("Oops", "We couldn't connect to the server!", "error");
                    }
                });
            }
        });
    }

    function showApprovalMessage() {
        debugger;
        $.ajax({
            type: "POST",
            url: '/Application/ApprovalMessage/',
            contentType: "application/json; charset=utf-8",
            dataType: "text",
            success: function (response) {
                window.location.href = "/Application/";
            }
        });
    }

    function rejectApplication(id) {
        swal({
            title: "Are you sure?",
            text: "Are you sure that you want to reject this application?",
            type: "warning",
            showCancelButton: true,
            confirmButtonText: "Yes, reject it!",
            confirmButtonColor: "#ec6c62"
        }).then((result) => {
            if (result) {
                $.ajax({
                    dataType: 'JSON',
                    type: 'POST',
                    async: false,
                    timeout: 600000,
                    url: "/Application/Reject?id=" + id,
                    success: function () {
                        showRejectMessage();
                    },
                    error: function () {
                        swal("Oops", "We couldn't connect to the server!", "error");
                    }
                });
            }
        });
    }

    function showRejectMessage() {
        $.ajax({
            type: "POST",
            url: '/Application/RejectMessage/',
            contentType: "application/json; charset=utf-8",
            dataType: "text",
            success: function (response) {
                window.location.href = "/Application/";
            }
        });
    }

    function requestMissingFiles(id) {
        swal({
            title: "Missing File Request",
            text: "Do you want to send an mail to the applicant about the missing files.",
            type: "warning",
            showCancelButton: true,
            confirmButtonText: "Yes!",
            confirmButtonColor: "#ec6c62"
        }).then((result) => {
            if (result) {
                $.ajax({
                    dataType: 'JSON',
                    type: 'POST',
                    async: false,
                    timeout: 600000,
                    url: "/Application/RequestMissingFiles?id=" + id,
                    success: function () {
                        showReqMissingFilesMessage();
                    },
                    error: function () {
                        swal("Oops", "We couldn't connect to the server!", "error");
                    }
                });
            }
        });
    }

    function showReqMissingFilesMessage() {
        $.ajax({
            type: "POST",
            url: '/Application/MissingFilesMessage/',
            contentType: "application/json; charset=utf-8",
            dataType: "text",
            success: function (response) {
                window.location.href = "/Application/";
            }
        });
    }

</script>
