@using Isik.SAMS.Models.Entity

@model SAMS_StudentApplications
@{
    ViewBag.Title = "Application Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    .dropdown:not(.custom-dropdown-icon) .dropdown-menu a.dropdown-item {
        border-radius: 5px;
        font-size: 11px;
        font-weight: 700;
        color: #888ea8;
        padding: 8px 16px 8px 8px;
        cursor: pointer;
    }

    .pull-left {
        color: black !important;
    }
</style>

<script>
    function isDefaultFunction(data) {
        if (data == 0) {
            return '<i style="color:rgb(185, 1, 0);margin-left:31%;" class="icon-minus"></i>'
        }
        else {
            return '<i style="color:rgb(41, 180, 0);margin-left:31%;" class="icon-ok center"></i>'
        }
    }

</script>
<div class="col-12">
    <div class="statbox widget box box-shadow">
        <div class="widget-header">
            <div class="cover">
                <div class="cover-head" style="background-image: url('/Assets/img/background2.jpg');">
                </div>
                <div class="cover-content">
                    <span class="cover-label">
                        <strong style="color:#304aca;text-transform:uppercase">@Model.StudentFirstName @Model.StudentLastName</strong>
                        <span>@Model.Email</span>
                    </span>

                    <span class="cover-label">
                        <strong>Program</strong>
                        <span>@Model.ProgramName</span>
                    </span>
                    <span class="cover-label">
                        <strong>Department</strong>
                        <span>@Model.DepartmentName</span>
                    </span>
                    <span class="cover-label">
                        <strong>Status</strong>
                        <span style="color:@(Model.Status == 1 || Model.Status == 3 || Model.Status == 7 ? "darkorange" : Model.Status == 2 || Model.Status == 4 || Model.Status == 6 || Model.Status == 9 ? "green" : Model.Status == 5 ? "red" : "blue"); font-weight:bold;">@Model.StatusName</span>
                    </span>
                    <span class="cover-label">

                        <strong>Application Date</strong>
                        <span>@Model.CreatedTime</span>

                    </span>
                    @if (Model.Status == 2 && Convert.ToInt32(Session["UserType"]) == 1)
                    {
                        <div class="pull-right">
                            <a class="btn btn-info btn-sm pull-right ml-1" href="@Url.Action("DownloadAllFiles", "Application", new { Id = Model.Id })">
                                <i class="icon-download-1"></i> Download the files
                            </a>
                        </div>
                    }
                    else if (Model.Status == 2 || Model.Status == 1 || Model.Status == 3)
                    {
                        <div class="pull-right">
                            <a class="btn btn-info btn-sm pull-right ml-1" href="@Url.Action("DownloadAllFiles", "Application", new { Id = Model.Id })">
                                <i class="icon-download-1"></i> Download the files
                            </a>
                            <button class="btn btn-danger btn-sm pull-right ml-1" onclick="rejectApplication('@(Model.Id)')">
                                <i class="icon-cancel-1"></i> Reject
                            </button>
                            @if (Convert.ToInt32(Session["UserType"]) == 2)
                            {
                                <button class="btn btn-warning btn-sm pull-right ml-1" data-task="Insert" data-method="GET" data-href="@Url.Action("MeetingLink","Application", new { Id = Model.Id })">
                                    <i class="icon-mail-1"></i> Send Meeting Link
                                </button>
                                <button class="btn btn-success btn-sm pull-right ml-1" data-task="Insert" data-method="GET" data-href="@Url.Action("Scholarship","Application", new { Id = Model.Id })">
                                    <i class="icon-thumbs-up-1"></i> Approve
                                </button>
                            }
                            else
                            {
                                <button class="btn btn-warning btn-sm pull-right ml-1" data-task="Insert" data-method="GET" data-href="@Url.Action("MissingFiles","Application", new { Id = Model.Id })">
                                    <i class="icon-mail-1"></i> Request Missing Files
                                </button>
                                <button class="btn btn-success btn-sm pull-right ml-1" onclick="approveApplication('@(Model.Id)');">
                                    <i class="icon-thumbs-up-1"></i> Approve
                                </button>
                            }
                        </div>
                    }
                    else if (Model.Status == 4 && Convert.ToInt32(Session["UserType"]) == 1)
                    {
                        <div class="pull-right">
                            <a class="btn btn-info btn-sm pull-right ml-1" href="@Url.Action("DownloadAllFiles", "Application", new { Id = Model.Id })">
                                <i class="icon-download-1"></i> Download the files
                            </a>
                            <button class="btn btn-warning btn-sm pull-right ml-1" onclick="requestPayment('@(Model.Id)');">
                                <i class="icon-money-1"></i> Request Payment
                            </button>
                            <button class="btn btn-danger btn-sm pull-right ml-1" onclick="rejectApplication('@(Model.Id)')">
                                <i class="icon-cancel-1"></i> Reject
                            </button>
                        </div>
                    }
                    else if (Model.Status == 9 && Convert.ToInt32(Session["UserType"]) == 1)
                    {
                        <div class="pull-right">
                            <a class="btn btn-info btn-sm pull-right ml-1" href="@Url.Action("DownloadAllFiles", "Application", new { Id = Model.Id })">
                                <i class="icon-download-2"></i> Download the files
                            </a>
                            <button class="btn btn-warning btn-sm pull-right ml-1" onclick="requestPayment('@(Model.Id)');">
                                <i class="icon-money-2"></i> Request Payment
                            </button>
                            <button class="btn btn-danger btn-sm pull-right ml-1" onclick="rejectApplication('@(Model.Id)')">
                                <i class="icon-cancel-2"></i> Reject
                            </button>
                            <button class="btn btn-success btn-sm pull-right ml-1" onclick="enroll('@(Model.Id)');">
                                <i class="icon-ok-2"></i> Enroll
                            </button>
                        </div>
                    }
                </div>
            </div>
