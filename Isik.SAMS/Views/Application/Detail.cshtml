@using Isik.SAMS.Models.Entity

@model SAMS_StudentApplications
@{
    ViewBag.Title = "Application Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    .dropdown:not(.custom-dropdown-icon) .dropdown-menu a.dropdown-item {
        border-radius: 5px;
        font-size: 11px;
        font-weight: 700;
        color: #888ea8;
        padding: 8px 16px 8px 8px;
        cursor: pointer;
    }
</style>

<script>
    function isDefaultFunction(data) {
        if (data == 0) {
            return '<i style="color:rgb(185, 1, 0);margin-left:31%;" class="icon-minus"></i>'
        }
        else {
            return '<i style="color:rgb(41, 180, 0);margin-left:31%;" class="icon-ok center"></i>'
        }
    }

</script>
<div class="col-12">
    <div class="statbox widget box box-shadow">
        <div class="widget-header">
            <div class="cover">
                <div class="cover-head" style="background-image: url('/Assets/img/account1.jpg');">
                    <span class="cover-label" style="font-size:18px;background:#fff;padding:5px 10px;border-radius:2px;float:right;">
                    </span>
                </div>
                <div class="cover-content">
                    <span class="cover-label">
                        <strong style="color:#304aca;text-transform:uppercase">@Model.StudentFirstName @Model.StudentLastName</strong>
                        <span>@Model.Email</span>
                    </span>

                    <span class="cover-label">
                        <strong>Program</strong>
                        <span>@Model.ProgramName</span>
                    </span>
                    <span class="cover-label">
                        <strong>Department</strong>
                        <span>@Model.DepartmentName</span>
                    </span>
                    <span class="cover-label">
                        <strong>Status</strong>
                        <span>@Model.StatusName</span>
                    </span>
                    <span class="cover-label">

                        <strong>Application Date</strong>
                        <span>@Model.CreatedTime</span>

                    </span>
                    @if (Model.Status == 2 || Model.Status == 1 || Model.Status == 3)
                    {
                        <div class="pull-right">
                            <a class="btn btn-info btn-sm pull-right ml-1" data-task="Insert" data-method="GET" data-href="@Url.Action("Insert", "Account")">
                                <i class="icon-download"></i> Download the files
                            </a>
                            <button class="btn btn-danger btn-sm pull-right ml-1" onclick="rejectApplication('@(Model.Id)')">
                                <i class="icon-cancel-2"></i> Reject
                            </button>
                            @if (Convert.ToInt32(Session["UserType"]) == 2)
                            {
                                <button class="btn btn-success btn-sm pull-right ml-1" data-task="Insert" data-method="GET" data-href="@Url.Action("MeetingLink","Application", new { Id = Model.Id })">
                                    <i class="icon-ok-2"></i> Send Meeting Link
                                </button>
                                <button class="btn btn-success btn-sm pull-right ml-1" data-task="Insert" data-method="GET" data-href="@Url.Action("Scholarship","Application", new { Id = Model.Id })">
                                    <i class="icon-ok-2"></i> Approve
                                </button>
                            }
                            else
                            {
                                if (ViewBag.isFileMissing == "True")
                                {
                                    <button class="btn btn-success btn-sm pull-right ml-1"onclick="requestMissingFiles('@(Model.Id)');">
                                        <i class="icon-ok-2"></i> Request Missing Files
                                    </button>
                                }
                                else
                                {
                                    <button class="btn btn-success btn-sm pull-right ml-1" onclick="approveApplication('@(Model.Id)');">
                                        <i class="icon-ok-2"></i> Approve
                                    </button>
                                }


                            }
                        </div>
                    }
                </div>
            </div>
        </div>
        <div class="widget-content widget-content-area animated-underline-content">
            <ul class="nav nav-tabs" id="simpletab" role="tablist">
                <li class="nav-item">
                    <a class="nav-link active" id="general-tab" data-toggle="tab" href="#general" role="tab" aria-controls="general" aria-selected="true">Personal Information</a>
                </li>
                <li class="nav-item" data-href="/EMP/VWEMP_Leave/DataSource">
                    <a class="nav-link" id="leave-tab2" data-toggle="tab" href="#leave" role="tab" aria-controls="leave" aria-selected="false">Educational Information</a>
                </li>
                <li class="nav-item pull-right">
                    <a class="nav-link" id="employeeFiles-tab2" data-toggle="tab" href="#employeeFiles" role="tab" aria-controls="employeeFiles" aria-selected="false">Documents</a>
                </li>


            </ul>
            <div class="tab-content" id="simpletabContent">
                <div class="tab-pane fade show active" id="general" role="tabpanel" aria-labelledby="general-tab">
                    <div>
                        <div class="row">
                            <div class="col-md-6">
                                <div class="clearfix">
                                    <div class="pull-left"><strong>Name</strong></div>
                                    <div class="pull-right">@Model.StudentFirstName </div>
                                </div>
                                <hr>
                                <div class="clearfix">
                                    <div class="pull-left"><strong>Surname</strong></div>
                                    <div class="pull-right">@Model.StudentLastName</div>
                                </div>

                                <hr>
                                <div class="clearfix">
                                    <div class="pull-left"><strong>Gender</strong></div>
                                    <div class="pull-right">@Model.Gender</div>
                                </div>
                                <hr>
                                <div class="clearfix">
                                    <div class="pull-left"><strong>Date of Birth</strong></div>
                                    <div class="pull-right">@Model.DateofBirth</div>
                                </div>
                                <hr>
                                <div class="clearfix">
                                    <div class="pull-left"><strong>City of Birth</strong></div>
                                    <div class="pull-right">@Model.CityofBirth</div>
                                </div>
                                <hr>
                                <div class="clearfix">
                                    <div class="pull-left"><strong>Passport Number</strong></div>
                                    <div class="pull-right">@Model.PassportNumber</div>
                                </div>
                                <hr>
                                <div class="clearfix">
                                    <div class="pull-left"><strong>Country of Birth</strong></div>
                                    <div class="pull-right">@Model.Country</div>
                                </div>
                                <hr>
                                <div class="clearfix">
                                    <div class="pull-left"><strong>Citizenship</strong></div>
                                    <div class="pull-right">@Model.Citizenship</div>
                                </div>
                                <hr>
                            </div>
                            <div class="col-md-6">

                                <div class="clearfix">
                                    <div class="pull-left"><strong>Phone</strong></div>
                                    <div class="pull-right">@Model.PhoneNumber</div>
                                </div>
                                <hr>
                                <div class="clearfix">
                                    <div class="pull-left"><strong>Adress </strong></div>
                                    <div class="pull-right">@Model.Address</div>
                                </div>
                                <hr>
                                <div class="clearfix">
                                    <div class="pull-left"><strong>Country of Residence</strong></div>
                                    <div class="pull-right">@Model.Country</div>
                                </div>
                                <hr>
                                <div class="clearfix">
                                    <div class="pull-left"><strong>Father's Name</strong></div>
                                    <div class="pull-right">@Model.FatherName</div>
                                </div>
                                <hr>
                                <div class="clearfix">
                                    <div class="pull-left"><strong>Mother's Name</strong></div>
                                    <div class="pull-right">@Model.MotherName</div>
                                </div>
                                <hr>

                            </div>
                        </div>

                    </div>
                </div>
                <div class="tab-pane fade" id="leave" role="tabpanel" aria-labelledby="leave-tab">
                    <div>
                        @if (Model.ProgramId == 8002)
                        {
                            <div class="row">
                                <div class="col-md-6">

                                    <div class="clearfix">
                                        <div class="pull-left"><strong>Blue Card Owner (Turkish Applicants Only)</strong></div>
                                        <div class="pull-right">@Model.BlueCardOwner</div>
                                    </div>
                                    <hr>
                                    <div class="clearfix">
                                        <div class="pull-left"><strong>Dual Citizenship</strong></div>
                                        <div class="pull-right">@Model.DualCitizenship</div>
                                    </div>

                                    <hr>
                                    <div class="clearfix">
                                        <div class="pull-left"><strong>Previously studied at a university</strong></div>
                                        <div class="pull-right">@Model.IsGradFromUni</div>
                                    </div>
                                    <hr>
                                </div>
                                <div class="col-md-6">

                                    <div class="clearfix">
                                        <div class="pull-left"><strong>Secondary High Name</strong></div>
                                        <div class="pull-right">@Model.HighSchoolName</div>
                                    </div>
                                    <hr>
                                    <div class="clearfix">
                                        <div class="pull-left"><strong>Graduation Year of High School</strong></div>
                                        <div class="pull-right">@Model.HighSchoolGradYear</div>
                                    </div>
